{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true,
    "tsConfigPath": "tsconfig.json",
    "webpack": false,
    "assets": [],
    "watchAssets": false
  },
  "projects": {
    "infoteam-idp": {
      "type": "library",
      "root": "libs/infoteam-idp",
      "entryFile": "index",
      "sourceRoot": "libs/infoteam-idp/src",
      "compilerOptions": {
        "tsConfigPath": "libs/infoteam-idp/tsconfig.lib.json"
      }
    }
  }
}
